{
    "name": "Captcha Solver Extension",
    "description": "Captcha Solver Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab", "contextMenus"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ]
  }